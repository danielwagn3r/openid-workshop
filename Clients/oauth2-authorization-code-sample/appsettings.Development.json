{
  "DetailedErrors": true,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Azure": "Information",
        "IdentityModel": "Warning",
        "Duende": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    },
    "Properties": {
      "ApplicationName": "WebClient"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
